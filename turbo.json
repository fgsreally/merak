{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputs": ["dist/**", ".vitepress/dist/**"]
    },
    "dev": {
      "cache": false
    },
    "test:e2e":{
      "dependsOn": ["dev:example"],
      "cache": false
    }
  }
}

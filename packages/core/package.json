{
  "name": "merak-core",
  "version": "1.0.0-rc.11",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./loader": {
      "import": "./dist/loaders/index.mjs",
      "require": "./dist/loaders/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "loader": [
        "dist/loaders/index.d.ts"
      ]
    }
  },
  "scripts": {
    "serve": "http-server ./dist --cors -p 3000",
    "dev": "tsup --watch",
    "build": "tsup && npm run build:prod && npm run build:dev",
    "build:prod": "cross-env PROD=true tsup",
    "build:dev": "cross-env DEV=true tsup"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "merak-compile": "workspace:*",
    "tsup": "^6.5.0"
  }

}
